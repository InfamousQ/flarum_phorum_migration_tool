{
	"name": "infamousq/flarum-phorum-migration-tool",
	"description": "Flarum extension for importing Phorum forum database to existing Flarum installation",
	"type": "flarum-extension",
	"require": {
		"flarum/core": ">1.1",
		"flarum/sticky": ">1.1",
		"flarum/lock": ">1.1"
	},
	"autoload": {
		"psr-4": {"InfamousQ\\FlarumPhorumMigrationTool\\": "src/"}
	},
	"extra": {
		"flarum-extension": {
			"title": "Phorum migration tool",
			"icon": {
				"name": "fas fa-smile",
				"backgroundColor": "#238c59",
				"color": "#fff"
			}
		}
	}
}
